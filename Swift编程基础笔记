 private var squareArea:Double = 0//æ­£æ–¹å½¢çš„é¢ç§¯\n    \n    private var original_radius:Double = -1 //å‚¨å­˜æœ€æ–°åŠå¾„ //å­˜å‚¨æœ€æ–°åŠå¾„ï¼Œå¼€å§‹è®¾ç½®ä¸º-1\n    \n    var radius:Double//åŠå¾„\n    {\n        set(radius)\n        {\n            circleArea = Double.pi * (radius * radius )\n//            Ï€ * rå¹³æ–¹\n            let diam:Double = radius * 2\n            squareArea = diam * diam   //4piæ–¹\n            \n            original_radius = radius  //åŠå¾„\n           \n        }\n        get\n        {\n            return original_radius\n        }\n    }\n    \n    init(radius:Double)\n    {\n        self.radius = radius\n    }\n    \n    func getCircleArea() -> Double\n    {\n        return self.circleArea\n    }\n    \n    func getSquareArea() -> Double\n    {\n        return self.squareArea\n    }\n \n}\n \nvar area = GraphArea(radius:10) //åœ¨åˆ›å»ºGraphAreaå®ä¾‹çš„æ—¶ä¼ ä¸€ä¸ªåŠå¾„å‚æ•°\n//   åœ¨initï¼ˆï¼‰åˆå§‹åŒ–æ„é€ å™¨é‡Œé¢\nprint("åŠå¾„ = " + String(area.radius))  //è¿”å›areaçš„åŠå¾„\nprint("åœ†å½¢é¢ç§¯ = " + String(area.getCircleArea()))\nprint("æ­£æ–¹å½¢é¢ç§¯ = " + String(area.getSquareArea()))\n \nprint("-------------------- - -- -  - --------------------------")\narea.radius = 20 //ä¿®æ”¹åŠå¾„ï¼Œæ¯æ¬¡é‡è®¾åŠå¾„å€¼çš„æ—¶å€™ï¼Œsetä»£ç å—éƒ½ä¼šè¢«æ‰§è¡Œï¼Œä¹Ÿå°±æ˜¯æ‰€æœ‰é¢ç§¯éƒ½ä¼šè¢«é‡æ–°è®¡ç®—\nprint("åŠå¾„ = " + String(area.radius))  //è¿”å›areaçš„åŠå¾„\nprint("åœ†å½¢é¢ç§¯ = " + String(area.getCircleArea()))\nprint("æ­£æ–¹å½¢é¢ç§¯ = " + String(area.getSquareArea()))\n
